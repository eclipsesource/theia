{
	"name": "theia-performance-test-plugin",
	"publisher": "st",
	"displayName": "performance-plugin",
	"license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "theia.ptest.readsingle.plugin",
				"title": "Execute File Reading Performance Tests (VSCode/Theia plugin)"
			}		
		]
	},
	"scripts": {
		"prepare": "yarn run build",
		"compile": "tsc -p ./",
		"build": "yarn compile && yarn symlink",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"symlink": "symlink-dir . ../plugins/theia-performance-test-plugin"
	},

	"devDependencies": {
		"theia-performance-test":"0.0.1",
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "12.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3",
		"symlink-dir": "5.0.0"	}

}